{
    "manifest_version": 3,
    "name": "Sniff",
    "version": "1.4",
    "description": "Simply sniffs out m3u8 links.",
    "permissions": ["storage", "webRequest", "webRequestBlocking", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "icons": {
      "128": "icon.png"
    }
  }
  